  "bugs": {
    "url": "https://github.com/jbowwww/file-monger/issues"
  },
  "homepage": "https://github.com/jbowwww/file-monger#readme",
  "dependencies": {
    "async-exit-hook": "^2.0.1",
    "debug": "^4.4.0",
    "deep-object-diff": "^1.1.9",
    "mongodb": "^6.5.0",
    "music-metadata": "^7.13.3",
    "systeminformation": "^5.25.11",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/async-exit-hook": "^2.0.2",
    "@types/debug": "^4.1.12",
