{
  "name": "file-monger",
  "version": "1.0.0",
  "description": "Another attempt at a distributed file-mongering tool.",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build ; node --enable-source-maps ./dist/cli.js",
    "debug": "npm run build ; node --enable-source-maps --inspect-brk ./dist/cli.js",
    "dev": "node --enable-source-maps ./dist/cli.js",
    "help": "node dist/cli.js --help",
