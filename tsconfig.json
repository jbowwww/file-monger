{
    "compilerOptions": {
        "target": "ESNext",
        "module": "NodeNext",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node16",
        "sourceMap": true,
        "rootDir": ".",
        "outDir": "dist/",
        "allowSyntheticDefaultImports": true,
        "lib": [ ],
        "skipLibCheck": true,
    },
    "include": [
        "src/",
        "test/",
    ],
    "exclude": [
        "node_modules/**/",
        "da_isolated/**/",
        ".vscode",
    ]
}
